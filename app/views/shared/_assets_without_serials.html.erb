
<h2 class="small-fonts-header">Equipment Unassigned</h2>
<table class="table table-bordered table-condensed">
	<th class="small-fonts">
        Machine
    </th>
    <th class="small-fonts">
    	Install Date
    </th>
  		<% for asset in @assets_without_serials %>
	   <tr>
    		<td class="small-fonts">
                <%= link_to asset.name, edit_workorder_path(asset.workorder) %>
            </td>
    		<td class="small-fonts">
    			<% unless asset.workorder.wo_date.nil? %>
    				<%= asset.workorder.wo_date.strftime("%B %d, %Y at %I:%M %p") %>
    			<% else %>
    				Unscheduled
    			<% end %>
            </td>
	   </tr>
	<% end %>
</table>
