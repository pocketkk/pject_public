BEGIN:VCALENDAR
METHOD:PUBLISH
VERSION:2.0
PRODID:-//Workorder Machine/Time-Off//EN
X-WR-CALNAME:Workorder Machine
CALSCALE:GERGORIAN
<% @users.each do |user| %>
    <% user.day_offs.each do |day_off| %>
BEGIN:VEVENT
UID:<%=day_off.id%>
SUMMARY:<%= day_off.user.name.titleize %> | <%= day_off.do_type %>
DTSTART:<%= day_off.start_date.strftime("%Y%m%d") %>
DTEND:<%= day_off.end_date.strftime("%Y%m%d") %>
END:VEVENT
    <%end%>
<% end %>
END:VCALENDAR
