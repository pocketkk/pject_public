<%= form_for @asset do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :serial %><br />
    <%= f.text_field :serial %>
  </p>
  <p>
    <%= f.label :workorder_id %><br />
    <%= f.number_field :workorder_id %>
  </p>
  <p><%= f.submit %></p>
<% end %>
